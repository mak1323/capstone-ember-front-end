<li>
  <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#{{log.id}}readModal">
    {{log.title}}
  </button>
  {{!-- <a href="#" data-toggle="modal" data-target="#readModal">{{log.title}}</a> --}}
  <button class="crudbtn" {{action 'deleteLog'}}>&#9762;</button>
  {{!-- {{#link-to 'log-edit-form' log}}<button class="crudbtn" >&#9881;</button>{{/link-to}} --}}
</li>

<div class="modal fade" id="{{log.id}}readModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">{{log.title}}</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <p>{{log.log}}</p>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>
