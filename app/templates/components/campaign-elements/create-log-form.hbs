<div class="createLogDiv box">
<h4>Start a new Log for {{campaign.name}}</h4>
<form {{action 'createLog' on='submit'}}>
  {{input
    placeholder="New Log title"
    value=newLog.title
  }}

  {{!-- {{input
  placeholder=campaign.id
  value=newLog.campaign_id
}} --}}

  {{input
    placeholder="Log--255 characters or less."
    value=newLog.log
  }}
  <button type="submit">Post New Log &#9874;</button>
</form>
</div>
